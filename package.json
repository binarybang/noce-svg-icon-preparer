{
  "name": "icon-preparator",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "description": "A utility to prepare SVG icons for use in icon set components.",
  "devDependencies": {
    "@tsconfig/node14": "^1.0.1",
    "@types/commander": "^2.12.2",
    "@types/jsdom": "^16.2.13",
    "@types/node": "^16.10.1",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "typescript": "^4.4.3"
  },
  "files": [
    "./dist/**/*"
  ],
  "scripts": {
    "build": "yarn run build:dev",
    "build:dev": "yarn run version:output && tsc -p tsconfig.dev.json",
    "build:prod": "yarn run version:output && tsc -p tsconfig.prod.json",
    "version:output": "node scripts/extract-pkg-info.js",
    "lint": "yarn run eslint .",
    "lint:fix": "yarn run eslint . --fix"
  },
  "bin": {
    "prepare-icons": "./dist/index.js"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^8.2.0",
    "jsdom": "^17.0.0"
  }
}
